{
  "kind": "collectionType",
  "collectionName": "todos",
  "info": {
    "name": "todo"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "todoText": {
      "type": "string",
      "required": true
    },
    "done": {
      "type": "boolean"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
